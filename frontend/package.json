{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm install && npx tsc --project ./tsconfig.json && npx tailwindcss -i ./src/input.css -o ./dist/output.css && cp ./index.html ./dist/index.html && cp -r ./src/pages ./dist/pages",
