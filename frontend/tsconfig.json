{
    "compilerOptions": {
        "target": "ES6",
        "module": "ESNext",
		"moduleResolution": "Bundler",
        "outDir": "./dist",
        "rootDir": "./src",
		"paths": {
            "undici-types": ["./node_modules/undici-types"]
        },
        "strict": true,
        "esModuleInterop": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}